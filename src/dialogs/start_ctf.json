{
    "title": "Start a CTF",
    "callback_id": "start-ctf",
    "submit_label": "Submit",
    "elements": [
        {
            "label": "Name",
            "type": "text",
            "name": "name",
            "hint": "Full name of CTF"
        },
        {
            "label": "Channel Prefix",
            "type": "text",
            "name": "chan_prefix",
            "hint": "Prefix to use for challenge channels"
        },
        {
            "label": "IRC URI",
            "type": "text",
            "name": "irc_uri",
            "hint": "Use ircs:// instead of irc:// if ssl is required.",
