        },
        {
            "label": "IRC Channel",
            "type": "text",
            "name": "irc_chan",
            "hint": "Channel name for CTF IRC.",
            "optional": true
        },
        {
            "label": "IRC Nickname",
            "type": "text",
            "name": "irc_nick",
            "hint": "Nickname for use in the IRC chat",
            "value": "SCC",
            "optional": true
        }
    ]
